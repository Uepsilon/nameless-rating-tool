!!!
%html
  %head
    %meta{ content: 'text/html; charset=UTF-8', http: { equiv: 'Content-Type' }}/
    %meta{ content: 'width=device-width, initial-scale=1', name: 'viewport' }/
    %link{ href: asset_pack_path('images/favicons/android-icon-192x192.png'), rel: 'icon', sizes: '192x192', type: 'image/png'}/
    %link{ href: asset_pack_path('images/favicons/favicon-32x32.png'), rel: 'icon', sizes: '32x32', type: 'image/png'}/
    %link{ href: asset_pack_path('images/favicons/favicon-96x96.png'), rel: 'icon', sizes: '96x96', type: 'image/png'}/
    %link{ href: asset_pack_path('images/favicons/favicon-16x16.png'), rel: 'icon', sizes: '16x16', type: 'image/png'}/
    %link{ href: asset_pack_path('images/favicons/apple-icon-57x57.png'), rel: 'apple-touch-icon', sizes: '57x57' }/
    %link{ href: asset_pack_path('images/favicons/apple-icon-60x60.png'), rel: 'apple-touch-icon', sizes: '60x60' }/
    %link{ href: asset_pack_path('images/favicons/apple-icon-72x72.png'), rel: 'apple-touch-icon', sizes: '72x72' }/
    %link{ href: asset_pack_path('images/favicons/apple-icon-76x76.png'), rel: 'apple-touch-icon', sizes: '76x76' }/
