    %link{ href: asset_pack_path('images/favicons/apple-icon-152x152.png'), rel: 'apple-touch-icon', sizes: '152x152' }/
    %link{ href: asset_pack_path('images/favicons/apple-icon-180x180.png'), rel: 'apple-touch-icon', sizes: '180x180' }/

    %title RatingTool
    = csrf_meta_tags
    = csp_meta_tag
    = javascript_pack_tag 'application', 'data-turbolinks-track': 'reload'
    = stylesheet_pack_tag 'application'

    %link{ href: 'https://fonts.googleapis.com/icon?family=Material+Icons', rel: 'stylesheet' }
  %body
    = render '/layouts/navbar'
    %section.section
      = yield
